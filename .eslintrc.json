{
    "root": true,
    "env": {
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "jest"
    ],
    "settings": {
        "import/core-modules": [
            "aws-sdk"
        ]
    },
    "rules": {
        "no-console": 0,
        "no-bitwise": 0,
        "max-len": [
            "error",
            180
        ]
    }
}